{
  "appId": "yessy.app",
  "appName": "Yessy",
  "bundledWebRuntime": false,
  "npmClient": "npm",
  "webDir": "www",
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 60000,
      "launchAutoHide": true,
      "launchFadeOutDuration": 500,
      "backgroundColor": "#3805c6",
      "androidSplashResourceName": "splash",
      "androidScaleType": "CENTER_CROP"
    }
  },
  "server": {
    "androidScheme": "https",
    "hostname": "yessy.app"
  }
}
